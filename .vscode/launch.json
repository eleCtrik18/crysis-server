{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "FastAPI with Poetry",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/src/main.py",
        "args": [
          "src.main:app",
          "--reload"
        ],
        "cwd": "${workspaceFolder}",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "preLaunchTask": "poetry_install_and_shell"
      }
    ],
    "compounds": []
  }
  